import com.sun.glass.ui.Pixels.Format;


public class Events {
	
	public static void newProcessEvent(long eventTime, float cPU_Time){
		System.out.println("Time:	" + formatTime(eventTime) + " Event 'New Process': pid=0 totalCPU=" + formatTime((long)cPU_Time));
	}
	
	public static float formatTime(long time){
		float temp = (float) time;
		return temp;
	}
}
